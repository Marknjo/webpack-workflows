{
	"name": "webpack-workflows",
	"version": "1.0.0",
	"description": "Will help to build any JavaScript oriented project with the support of SASS/CSS fast. Also, since I have omitted the package.json file, it means that your initial build will always contain all the latest dependancied at the time of clone.",
	"main": "postcss.config.js",
	"scripts": {
		"start": "webpack-dev-server --mode development",
		"smart:start": "webpack-dashboard -m webpack-dev-server --mode development",
		"dev": "cross-env NODE_ENV=development webpack --mode development",
		"build": "cross-env NODE_ENV=production webpack --mode production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Marknjo/webpack-workflows.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Marknjo/webpack-workflows/issues"
	},
	"homepage": "https://github.com/Marknjo/webpack-workflows#readme",
	"devDependencies": {
		"@babel/core": "^7.13.14",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/preset-env": "^7.13.12",
		"autoprefixer": "^10.2.6",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.2.6",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^5.3.1",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"optimize-css-assets-webpack-plugin": "^6.0.0",
		"postcss-loader": "^5.3.0",
		"purgecss-webpack-plugin": "^4.0.3",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.3.0",
		"webpack": "^5.28.0",
		"webpack-cli": "^3.3.12",
		"webpack-dashboard": "^3.3.1",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^4.2.2"
	}
}
